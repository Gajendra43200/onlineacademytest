<!-- app/views/completed_subjects/new.html.erb -->
<%= form_with(model: [@student,@subject, @completed_subject], local: true) do |form| %>
    <%= form.hidden_field :subject_id, value: @subject.id %><br>
    <%= form.hidden_field :student_id, value: session[:student_id]%><br>
    <%= form.submit 'Create Completed Subject' %>
<% end %>
